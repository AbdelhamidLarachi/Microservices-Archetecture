{
  "name": "vue-2-boilerplate",
  "version": "0.0.0",
  "description": "A boilerplate for medium to large Vue 2 single page applications",
  "author": "Peter van Meijgaard <petervanmeijgaard@hotmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "e2e": "vue-cli-service e2e",
    "test": "vue-cli-service test"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "register-service-worker": "^1.7.2",
    "standard": "^16.0.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/petervmeijgaard/vue-2-boilerplate/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://github.com/petervmeijgaard/vue-2-boilerplate#readme",
  "keywords": [
    "Vue",
    "Next",
    "Boilerplate",
    "Webpack",
    "Lint",
    "Medium",
    "Large",
    "SPA",
    "Single",
    "Page",
    "Application"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petervmeijgaard/vue-2-boilerplate.git"
  }
}
